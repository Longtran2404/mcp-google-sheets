  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Longtran2404/mcp-google-sheets.git"
  },
  "bugs": {
    "url": "https://github.com/Longtran2404/mcp-google-sheets/issues"
  },
  "homepage": "https://github.com/Longtran2404/mcp-google-sheets#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "googleapis": "^128.0.0",
    "google-auth-library": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
